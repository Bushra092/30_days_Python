   "source": [
    "def factorial(num):\n",
    "    if num == 0 or num == 1:\n",
    "        return 1\n",
    "\n",
    "    return num *  factorial(num-1)    \n",
    "\n",
    "\n",
    "print(factorial(3))\n"
   ]
